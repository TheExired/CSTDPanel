		local function SearchFrame()

			local function NewCorner(p,rad)
				local corner = Instance.new("UICorner")
				corner.Parent = p
				corner.CornerRadius = rad
			end

			local function NewGrad(p,cl)
				local Grad = Instance.new("UIGradient")
				Grad.Parent = p
				Grad.Color = cl
				Grad.Rotation = 90
			end
			local ListFrame1 = Instance.new("Frame")
			ListFrame1.Parent = TowersFrame
			ListFrame1.Name = "SearchFrame"
			ListFrame1.Size = UDim2.new(0.56,0,0.1,0)
			ListFrame1.Position = UDim2.new(0.405,0,0.01,0)
			ListFrame1.BackgroundTransparency = 1
			ListFrame1.ZIndex = 11

			local ListFrame2 = Instance.new("Frame")
			ListFrame2.Parent = ListFrame1
			ListFrame2.Name = "SearchFrame"
			ListFrame2.Size = UDim2.new(1,0,1,0)
			ListFrame2.Position = UDim2.new(0,0,0,0)
			ListFrame2.BackgroundColor3 = Color3.fromRGB(76,76,76)
			ListFrame2.ZIndex = 10

			NewCorner(ListFrame2, UDim.new(0,8))

			NewGrad(ListFrame2,ColorSequence.new(Color3.fromRGB(255,255,255),Color3.fromRGB(194,194,194)))

			local SearchBox = Instance.new("TextBox")
			SearchBox.Parent = ListFrame2
			SearchBox.Size = UDim2.new(0.77,0,1,0)
			SearchBox.Position = UDim2.new(0.23,0,0,0)
			SearchBox.BackgroundTransparency = 1
			SearchBox.ZIndex = 12
			SearchBox.PlaceholderText = "Search"
			SearchBox.TextColor3 = Color3.fromRGB(255,255,255)
			SearchBox.Text = ""
			SearchBox.Font = Enum.Font.SourceSansBold
			SearchBox.TextScaled = true
			SearchBox.TextStrokeTransparency = 0.75
			SearchBox.PlaceholderColor3 = Color3.fromRGB(255,255,255)
			SearchBox.ClearTextOnFocus = false
			SearchBox.Name = "SearchBox"

			local BG = Instance.new("Frame")
			BG.Parent = SearchBox
			BG.Name = "BG"
			BG.Size = UDim2.new(1,0,1,0)
			BG.Position = UDim2.new(0,0,0,0)
			BG.BackgroundColor3 = Color3.fromRGB(100,100,100)
			BG.ZIndex = 11

			NewCorner(BG, UDim.new(0,8))

			NewGrad(BG,ColorSequence.new(Color3.fromRGB(255,255,255),Color3.fromRGB(194,194,194)))















			local ResetButton = Instance.new("TextButton")
			ResetButton.Parent = ListFrame2
