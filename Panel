			local OpenCloseButton = Instance.new("TextButton")
			OpenCloseButton.Parent = ListFrame1
			OpenCloseButton.Size = UDim2.new(0.1,0,1,0)
			OpenCloseButton.Position = UDim2.new(0,0,0.5,0)
			OpenCloseButton.AnchorPoint = Vector2.new(0,0.5)
			OpenCloseButton.ZIndex = 14
			OpenCloseButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			OpenCloseButton.Text = ""
