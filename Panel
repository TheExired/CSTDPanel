			UIGrid.CellPadding = UDim2.new(0,3,0.005,0)
			UIGrid.CellSize = UDim2.new(0,100,0,100)
			UIGrid.HorizontalAlignment = Enum.HorizontalAlignment.Left
			UIGrid.FillDirection = Enum.FillDirection.Horizontal
			
