			ResetButton.Position = UDim2.new(0.115,0,0.5,0)
			ResetButton.AnchorPoint = Vector2.new(0,0.5)
			ResetButton.ZIndex = 13
			ResetButton.Name = "ResetButton"
			ResetButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			ResetButton.Text = ""

			local Icon = Instance.new("ImageLabel")
			Icon.Parent = ResetButton
			Icon.Image = "rbxassetid://1637592633"
			Icon.BackgroundTransparency = 1
