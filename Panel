--Tower Cycling

if game:GetService("Players").LocalPlayer.PlayerData.Loadout.Tower1.Value ~= "None" then
	game.ReplicatedStorage.Remotes.PlayerData:FireServer("Loadout","Equip", {Tower = "Bandit", Slot = 1})
	game.ReplicatedStorage.Remotes.PlayerData:FireServer("Loadout","Unequip", {Tower = "Bandit", Slot = 1})
	game.Players.LocalPlayer.PlayerGui.towerboothlol.Remote:FireServer(tostring(1), game:GetService("Players").LocalPlayer.PlayerData.Loadout.Tower1.Value)
end

